<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.semantic.semanticOrganizer.semanticcalendar" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".activities.Login"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activities.TaskManager"
            android:label="@string/title_activity_task_manager"
            android:theme="@style/Theme.AppCompat.Light" >
        </activity>
        <activity
            android:name=".activities.WriteTask"
            android:label="@string/title_activity_write_task" >
        </activity>
        <activity
            android:name=".activities.WriteEvent"
            android:label="@string/title_activity_write_event" >
            <intent-filter>
                <action android:name="android.intent.action.INSERT" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="text/plain" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ViewTaskActivity"
            android:label="@string/title_activity_view_task" >
        </activity>
        <activity
            android:name=".activities.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.AppCompat.Light" >
        </activity>
        <activity
            android:name=".activities.Write_Note_Activity"
            android:label="@string/title_activity_write__note_" >
        </activity>
        <activity
            android:name=".activities.NoteListActivity"
            android:label="@string/title_list_notes" >
        </activity>
        <activity
            android:name=".activities.ViewNoteActivity"
            android:label="@string/title_view_note" >
        </activity>
        <activity
            android:name=".activities.Write_Todo_Activity"
            android:label="@string/title_activity_write_todo" >
        </activity>
        <activity
            android:name=".activities.ViewToDoActivity"
            android:label="@string/title_view_todo" >
        </activity>
        <activity
            android:name=".activities.ViewCalendar"
            android:label="@string/title_activity_view_calendar" >
        </activity>
        <activity
            android:name=".ssupActivities.HelloActivity"
            android:label="Hello" >
        </activity>
        <activity
            android:name=".ssupActivities.MainActivity"
            android:label="Main" >
        </activity>
        <activity
            android:name=".activities.TutorialActivity"
            android:label="Tutorial" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.LandingActivity"
            android:label="@string/LandingActivity" >
        </activity>
        <activity
            android:name=".activities.AddTagActivity"
            android:label="@string/title_activity_add_tag" >
        </activity>
        <activity
            android:name=".activities.LandingActivityNew"
            android:label="@string/title_activity_landing_activity_new" >
        </activity>
        <activity
            android:name=".activities.AddNoteActivity"
            android:label="@string/title_activity_add_note" >
        </activity>
        <activity
            android:name=".activities.UpdateNoteActivity"
            android:label="@string/title_update_note" >
        </activity>
        <activity
            android:name=".activities.AddChecklistActivity"
            android:label="@string/title_activity_add_checklist" >
        </activity>
        <activity
            android:name=".activities.AddHabitActivity"
            android:label="@string/title_activity_add_habit" >
        </activity>
        <activity
            android:name=".activities.AddEventActivity"
            android:label="@string/title_activity_add_event" >
        </activity>
        <activity
            android:name=".activities.ListTagsActivity"
            android:label="@string/title_activity_list_tags" >
        </activity>
        <activity
            android:name=".activities.ListNotesActivity"
            android:label="@string/title_activity_list_notes" >
        </activity>
        <activity
            android:name=".activities.ListEventsActivity"
            android:label="@string/title_activity_list_events" >
        </activity>
        <activity
            android:name=".activities.ListChecklistsActivity"
            android:label="@string/title_activity_list_checklists" >
        </activity>
        <activity
            android:name=".activities.ListHabitsActivity"
            android:label="@string/title_activity_list_habits" >
        </activity>
    </application>

</manifest>
